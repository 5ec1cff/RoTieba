<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="PhotoAppBarStyle" parent="Widget.Material3.Toolbar">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Toolbar</item>
        <item name="colorOnPrimary">@color/white</item>
    </style>

    <style name="ThemeOverlay.App.Toolbar" parent="">
        <item name="colorControlNormal">@color/white</item>
    </style>

    <style name="Widget.RoTieba.FilledCardPrimary" parent="Widget.Material3.CardView.Filled">
        <item name="cardBackgroundColor">?attr/colorPrimaryContainer</item>
    </style>

    <style name="Widget.RoTieba.FilledCardSecondary" parent="Widget.Material3.CardView.Filled">
        <item name="cardBackgroundColor">?attr/colorSecondaryContainer</item>
    </style>

    <style name="Widget.RoTieba.SmallOutlinedButton" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
    </style>
